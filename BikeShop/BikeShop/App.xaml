<Application x:Class="BikeShop.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BikeShop"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!--<Button x:Key="button">Hello, world!</Button>
        <SolidColorBrush x:Key="accentBrush" Color="Red"/>-->
        <ResourceDictionary>
            <Style x:Key="myWindow"  TargetType="Window">
                <Setter Property="Width" Value="1280"/>
                <Setter Property="Height" Value="720"/>
            </Style>
            <Style x:Key="myPage"  TargetType="Page">
                <Setter Property="Width" Value="1280"/>
                <Setter Property="Height" Value="720"/>
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>
